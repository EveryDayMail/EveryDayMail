{
    
    "manifest_version": 2 ,
    "name": "MailMail",
    "version": "1.1.0",
    
    "description": "메일 템플릿 관리 서비스",
    "permissions" : [
        "tabs",
        "contextMenus",
        "notifications",
        "storage"
    ],
    "icons": {
        "128": "icons/app_icon.png"
    },

    
    "sidebar_action": {
        "default_page": "index.html", 
        "default_icon": {
            "128" : "icons/sidebar_icon.png"
        },
        "default_title": "매일메일", 
        "use_navigation_bar": true 
    }   
    ,

  
    "background": {
        "scripts": [
        ]
    },
    "content_scripts": [
    ]
    
}